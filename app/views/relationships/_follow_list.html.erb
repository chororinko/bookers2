<% users.each do |user| %>
    <tr>
      <td><%= link_to(user_path(user)) do %>
        <%= image_tag(user.get_profile_image(100,100)) %>
        <% end %>
      </td>
      <td><%= user.name %></td>
      <td>フォロー数：<%= user.followings.count %></td>
      <td>フォロワー数：<%= user.followers.count %></td>
      <td><%= link_to 'Show', user_path(user.id) %></td>
    </tr>
<% end %>