<!--表示はできてる-->
<div>
  <h1>User info</h1>
  <%= image_tag @user.get_profile_image(100,100) %>
  <table>
    <tbody>
      <tr>
        <td>name</td>
        <td><%= @user.name %></td>
      </tr>
      <tr>
        <td>introduction</td>
        <td><%= @user.introduction %></td>
      </tr>
      <tr>
        <td><% if @user == current_user %>
          <p><%= link_to 'edit', edit_user_path(@user) %></p>
            <% end %>
        </td>
      </tr>
    </tbody>
  </table>
</div>