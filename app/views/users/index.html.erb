<main>
<h2>Users</h2>
<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= link_to(user_path(user)) do %>
        <%= image_tag(user.get_profile_image(100,100)) %>
        <% end %>
      </td>
      <td><%= user.name %></td>
      <td><%= link_to 'Show', user_path(user.id) %></td>
    </tr>
    <% end %>
  </tbody>
</table>
</main>